{
  "name": "js-starter",
  "version": "1.0.0",
  "description": "This is a basic js starter boot app.",
  "main": "index.js",
  "test": "echo \"Error: no test specified\" && exit 1",
  "scripts": {
    "prestart": "",
    "start": "npm-run-all --parallel start-app lint:watch test:watch start-mockapi",
    "start-app": "babel-node buildScripts/srcServer -- --environment=dev",
    "lint": "esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "poststart": "",
    "security-check": "nsp check",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "command-line-args": "^4.0.4",
    "compression": "^1.6.2",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.1.0",
    "json-schema-faker": "^0.5.0-rc3",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
